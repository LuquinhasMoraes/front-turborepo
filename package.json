    "@heroicons/react": "^1.0.6",
    "bootstrap": "^5.1.3",
    "correios-brasil": "^2.2.2",
    "frete": "^0.4.6",
    "mobx": "^6.5.0",
    "mobx-react-lite": "^3.3.0",
    "mobx-state-tree": "^5.1.3",
